page {
    background: #000;
    width: 100%;
    height: 100%;
    position: relative;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-flex: 2;
    flex: 2;
}

view {
    -webkit-box-flex: 1;
    flex: 1;
}

.icon-back {
    font-size: 50rpx;
}