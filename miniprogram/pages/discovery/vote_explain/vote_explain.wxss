.explain-bd {
    border-bottom: 1rpx solid #f4f4f8;
    padding: 10rpx 20rpx;
    margin: 10rpx 0;
    color: #666;
}

.explain-bd>view:first-child {
    color: #333;
    font-size: 1.2em;
}

.explain-bd>view:last-child {
    margin: 10rpx;
}

.explain-bd:last-child {
    border-bottom: none;
}

.bottom_but {
    position: fixed;
    z-index: 10;
    width: 100%;
    bottom: 0;
    left: 0;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-flex: 1;
    flex: 1;
    box-shadow: 0 0 6px 2px rgba(51,51,102,.1);
}

.bottom_but>navigator {
    display: inline-block;
    -webkit-box-flex: 1;
    flex: 1;
    width: 100%;
    text-align: center;
    height: 58rpx;
    line-height: 58rpx;
    margin: 15rpx 10rpx;
    color: #268cdd;
    border-radius: 10rpx;
}

.bottom_but>view {
    font-size: 9pt;
    color: #268cdd;
    position: relative;
    top: -10rpx;
}