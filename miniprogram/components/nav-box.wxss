.navBoxStyle {
    display: flex;
    -webkit-box-flex: 5;
    flex: 5;
    width: 100%;
    height: 55rpx;
    line-height: 55rpx;
    border-radius: 16rpx;
    overflow: hidden;
}

.navBoxStyle>view {
    -webkit-box-flex: 1;
    flex: 1;
    text-align: center;
    border-width: 0 1rpx 0 0;
    border-style: solid;
}

.navBoxStyle>view:last-child {
    border-width: 0 0 0 0;
}